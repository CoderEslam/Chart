<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doubleclick.chart">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Chart">
        <activity
            android:name=".hellocharts.samples.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".hellocharts.samples.LineChartActivity"
            android:label="@string/title_activity_line_chart" />
        <activity
            android:name=".hellocharts.samples.ColumnChartActivity"
            android:label="@string/title_activity_column_chart" />
        <activity
            android:name=".hellocharts.samples.PieChartActivity"
            android:label="@string/title_activity_pie_chart" />
        <activity
            android:name=".hellocharts.samples.BubbleChartActivity"
            android:label="@string/title_activity_bubble_chart" />
        <activity
            android:name=".hellocharts.samples.PreviewLineChartActivity"
            android:label="@string/title_activity_preview_line_chart" />
        <activity
            android:name=".hellocharts.samples.PreviewColumnChartActivity"
            android:label="@string/title_activity_preview_column_chart" />
        <activity
            android:name=".hellocharts.samples.ComboLineColumnChartActivity"
            android:label="@string/title_activity_combo_line_column_chart" />
        <activity
            android:name=".hellocharts.samples.LineColumnDependencyActivity"
            android:label="@string/title_activity_line_column_dependency" />
        <activity
            android:name=".hellocharts.samples.GoodBadChartActivity"
            android:label="@string/title_activity_good_bad" />
        <activity
            android:name=".hellocharts.samples.TempoChartActivity"
            android:label="@string/title_activity_tempo_chart" />
        <activity
            android:name=".hellocharts.samples.SpeedChartActivity"
            android:label="@string/title_activity_speed_chart" />
        <activity
            android:name=".hellocharts.samples.ViewPagerChartsActivity"
            android:label="@string/title_activity_view_pager_charts" />
        <activity
            android:name=".hellocharts.samples.AboutActivity"
            android:label="@string/title_activity_about" />

    </application>

</manifest>